{
    "name": "mohamadtsn/laravel-api-responder",
    "description": "This is my package laravel-api-responder",
    "keywords": [
        "mohamadtsn",
        "laravel-api-responder"
    ],
    "homepage": "https://github.com/mohamadtsn/laravel-api-responder",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamad Borzoyi",
            "email": "mohamad.borzoyi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/container": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Mohamadtsn\\Responder\\": "/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mohamadtsn\\Responder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mohamadtsn\\Responder\\ResponderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
